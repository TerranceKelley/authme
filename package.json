{
  "name": "authme",
  "version": "0.1.0",
  "description": "Nuxt module that sets event.context.user from Authelia/Caddy forward-auth headers and provides useUser() and GET /api/me",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.js",
      "default": "./dist/module.js"
    }
  },
  "main": "./dist/module.js",
  "types": "./dist/module.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc && cp -r src/runtime dist/"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "license": "MIT"
}
